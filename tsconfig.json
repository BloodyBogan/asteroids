{
	"include": [
		"src/**/*",
		"webpack.config.ts"
	],
	"exclude": [
		"node_modules/*",
		"public/*"
	],
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "es2017",
		"importsNotUsedAsValues": "remove",
		"isolatedModules": false,
		"esModuleInterop": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"src/*": [
				"src/*"
			]
		}
	}
}